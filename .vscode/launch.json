{
    "configurations": [{
    "name": "Python: Current File (Integrated Terminal)",
    "type": "python",
    "request": "launch",
    "program": "${file}",
    "console": "integratedTerminal"
},
{
    "name": "pydantlit-example",
    "type": "python",
    "request": "launch",
    "module": "streamlit",
    "env": {
        "DEVELOP": "true"
    },
    "args": [
        "run",
        "${workspaceFolder}/pydantlit/__init__.py",
        "--server.port",
        "8501"]
}
]
}
